<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="366.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="calendar.member.findPwd.FindPwdController">
   <children>
      <Label layoutX="102.0" layoutY="75.0" text="비밀번호 찾기" />
      <Label layoutX="54.0" layoutY="120.0" text="아이디 :" />
      <Label layoutX="67.0" layoutY="152.0" text="이름 :" />
      <Label layoutX="32.0" layoutY="187.0" text="핸드폰 번호 :" />
      <TextField fx:id="txtId" layoutX="112.0" layoutY="116.0" promptText="회원가입 시 입력한 아이디" />
      <TextField fx:id="txtName" layoutX="112.0" layoutY="148.0" promptText="회원가입 시 입력한 이름" />
      <TextField fx:id="txtNum" layoutX="112.0" layoutY="183.0" promptText="회원가입 시 입력한 번호" />
      <Button fx:id="btnFind" layoutX="161.0" layoutY="252.0" mnemonicParsing="false" onMouseClicked="#btnFindPwd" prefHeight="22.0" prefWidth="52.0" text="찾기" />
      <Button fx:id="btnClose" layoutX="221.0" layoutY="252.0" mnemonicParsing="false" onMouseClicked="#closeFindPwd" prefHeight="22.0" prefWidth="52.0" text="닫기" />
   </children>
</AnchorPane>
