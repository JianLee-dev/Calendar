<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="366.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="calendar.member.findId.FindIdController">
   <children>
      <Label layoutX="118.0" layoutY="74.0" text="아이디 찾기" />
      <Label layoutX="58.0" layoutY="141.0" text="이름 :" />
      <Label layoutX="23.0" layoutY="181.0" text="핸드폰 번호 :" />
      <TextField fx:id="txtName" layoutX="99.0" layoutY="137.0" promptText="회원가입 시 입력한 이름" />
      <TextField fx:id="txtNum" layoutX="99.0" layoutY="184.0" promptText="회원가입 시 입력한 번호" />
      <Button fx:id="btnFind" layoutX="161.0" layoutY="238.0" mnemonicParsing="false" onAction="#btnFindId" text="찾기" />
      <Button fx:id="btnClose" layoutX="213.0" layoutY="238.0" mnemonicParsing="false" onMouseClicked="#closeFindId" text="닫기" />
   </children>
</AnchorPane>
