<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="490.0" prefWidth="670.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="calendar.calendar.info.CalendarInfoController">
   <children>
      <TableView fx:id="tableView" layoutY="64.0" onMouseClicked="#click" prefHeight="363.0" prefWidth="670.0">
        <columns>
          <TableColumn fx:id="category" prefWidth="229.0" text="품목" />
          <TableColumn fx:id="name" prefWidth="229.0" text="이름" />
            <TableColumn fx:id="price" minWidth="0.0" prefWidth="231.0" text="가격" />
        </columns>
      </TableView>
      <Label fx:id="date" alignment="CENTER" layoutX="279.0" layoutY="14.0" prefHeight="45.0" prefWidth="150.0" text="Date">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="add" layoutX="238.0" layoutY="444.0" mnemonicParsing="false" onAction="#add" text="추가" />
      <Button fx:id="cancel" layoutX="389.0" layoutY="444.0" mnemonicParsing="false" onAction="#cancel" text="취소" />
      <Button fx:id="delete" layoutX="309.0" layoutY="444.0" mnemonicParsing="false" onAction="#delete" text="삭제" />
   </children>
</AnchorPane>
