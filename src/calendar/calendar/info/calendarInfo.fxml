      <Button fx:id="cancel" layoutX="418.0" layoutY="457.0" mnemonicParsing="false" onAction="#cancel" prefHeight="29.0" prefWidth="106.0" style="-fx-background-color: #ECDEC3;" text="cancle" textFill="#a3684d">
